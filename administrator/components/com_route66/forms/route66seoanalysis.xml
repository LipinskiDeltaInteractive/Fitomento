			<field name="keyword" required="true" class="input-xlarge" type="text" label="COM_ROUTE66_FOCUS_KEYWORD_LABEL" description="COM_ROUTE66_FOCUS_KEYWORD_DESC"/>
			<field name="preview" type="route66seopreview" label="COM_ROUTE66_SNIPPET_PREVIEW_LABEL" description="COM_ROUTE66_SNIPPET_PREVIEW_DESC"/>
			<field name="score" type="route66seoscore" default="0" label="COM_ROUTE66_SCORE_LABEL" description="COM_ROUTE66_SCORE_DESC"/>
			<field name="analysis" type="route66seoanalysis" label="COM_ROUTE66_ANALYSIS_LABEL" description="COM_ROUTE66_ANALYSIS_DESC"/>
		</fieldset>
	</fields>
